{
  "name": "nstyle-copy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:local": "vite",
    "build:local": "tsc && vite build",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint . --ext .tsx,.ts --fix",
    "preview:local": "vite preview",
    "pretier:check": "prettier . --check",
    "pretier:fix": "prettier . --write",
    "lp:check": "npm run lint && npm run pretier:check",
    "lp:fix": "npm run lint:fix && npm run pretier:fix",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server && copy functions\\api.js dist\\server",
    "preview": "cross-env NODE_ENV=production node server",
    "dev": "cross-env NODE_ENV=development node server.js",
    "netlify": "netlify dev -d public -f functions",
    "netlify:dev": "cross-env NODE_ENV=development netlify dev -d public -f functions",
    "netlify:preview": "cross-env NODE_ENV=production netlify dev -d public -f functions"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@netlify/functions": "^2.5.1",
    "@types/express": "^4.17.21",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "netlify-cli": "^17.15.3",
    "pkg": "^5.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "serverless-http": "^3.2.0",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
